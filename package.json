{
  "name": "my-wedding",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "npx dotenv -e ./.env prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint '*/**/*.{js,jsx,ts,tsx}'",
    "prettier:check": "prettier --check */**/*.{js,json,ts,tsx,css,md}",
    "prettier:fix": "prettier --write */**/*.{js,json,ts,tsx,css,md}",
    "migrate:dev": "npx dotenv -e ./.env.local -- prisma migrate dev",
    "migrate:test": "npx dotenv -e ./.env.test -- prisma migrate dev",
    "migrate:dev:reset": "npx dotenv -e ./.env.local -- prisma migrate reset",
    "migrate:test:reset": "npx dotenv -e ./.env.test -- prisma migrate reset",
    "seed": "npx dotenv -e ./.env.local ts-node back/seeders/main.ts",
    "db:push": "npx dotenv -e .env.local -- prisma db push",
    "migrate:reset": "npx dotenv -e .env.local -- prisma migrate reset",
    "prisma:generate": "npx dotenv -e .env.local -- prisma generate",
    "prisma:studio": "npx dotenv -e .env.local -- prisma studio",
    "production:build": "npx prisma generate && npx prisma migrate deploy && next build",
    "prepare": "husky"
  },
  "dependencies": {
    "@nextui-org/react": "^2.2.10",
    "@prisma/client": "^5.12.0",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "bcrypt": "^5.1.1",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "framer-motion": "^11.0.25",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "next": "14.1.4",
    "next-auth": "^4.24.7",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "prisma": "^5.12.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.0.1",
    "ts-node": "^10.9.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
